<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="dubbo-server"/>
    <!--不需要注册到服务注册中心-->
    <dubbo:registry address="N/A" />
    <!--<dubbo:registry address="zookeeper://192.168.216.128:2181" timeout="10000"/>-->
<!--    <dubbo:registry address="nacos://192.168.216.128:8848" timeout="10000"/>-->

    <dubbo:protocol name="dubbo" port="20880"/>

    <dubbo:service interface="org.example.HelloService" ref="helloService"/>

    <bean id="helloService" class="org.example.impl.HelloServiceImpl"/>


</beans>